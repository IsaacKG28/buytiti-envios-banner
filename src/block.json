{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "buytiti-banner-envios/buytiti-banner",
	"version": "0.1.0",
	"title": "Buytiti-banner-envios",
	"category": "widgets",
	"icon": "car",
	"description": "Este bloque sirve para insertar un banner de envios ",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "buytiti-banner",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"images": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"default": {},
				"properties": {
					"url": {
						"type": "string"
					},
					"link": {
						"type": "string"
					}
				}
			}
		}
	}
}
